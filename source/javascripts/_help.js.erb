var help = false

function helpT() {
  if (help) {
    document.body.classList.remove('help');
    document.cookie = 'helpOff';
    help = false;
  } else {
    document.body.classList.add('help');
    document.cookie = 'helpOn';
    help = true;
  }
}

if (document.cookie.split(' ')[1] == 'helpOn') {
  helpT();
}

console.log(document.cookie);

